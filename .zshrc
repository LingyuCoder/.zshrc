ZSH_THEME="wezm"
alias tb="tap build"
alias ts="tap server"
alias tl="tap link"
alias tw="tap watch"
alias kan="killall node"
alias gacs="gaa && gcmsg :smile:"
alias gacps="gaa && gcmsg :smile: && ggpush"
alias gcsmile="gcmsg :smile:"
alias zsh-reload="source ~/.zshrc"
alias ni="npm install"
alias nis="npm install --save"
alias nisd="npm install --save-dev"
alias nig="_ npm install -g"
alias tni="tnpm install"
alias tnis="tnpm install --save"
alias tnisd="tnpm install --save-dev"
alias tnig="_ npm install -g"
